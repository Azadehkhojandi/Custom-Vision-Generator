<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <add key="CustomVision_PredictionKey" value="" />
    <add key="CustomVision_TrainingKey" value="" />
    <add key="BingImageSearch_Key" value="" />
    <add key="BingImageSearch_Url" value="https://api.cognitive.microsoft.com/bing/v7.0/images/search" />
    <add key="ComputerVision_Key" value="" />
    <add key="ComputerVision_Url" value="https://westus2.api.cognitive.microsoft.com/vision/v1.0/generateThumbnail" />
    <add key="SizeOfImageSet" value="100" /> <!--recommend 100 images-->
    <add key="ProjectName" value="" /> <!-- leave empty to be autogenerated - fill in if you are training a model on images downloaded previously-->
    <add key="PerformImageDownload" value="true" /> <!-- if true images will be downloaded if false images previously downloaded into the project directories will be used-->
    <add key="TrainModel" value="true" /> <!-- if true training images will be uploaded and the model will be trained -->
    <add key="TestModel" value="true" /> <!-- if true test images will be uploaded and the model will be tested against the test images and the confusion matrix will be generated-->
    <add key="PerformImageAugmentation" value="true" /> <!-- if true training images will be augmented before uploading to be trained -->
    <add key="WidthHeight" value="299" /> <!-- width and height to crop the images to -->
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
      
      <dependentAssembly>
        <assemblyIdentity name="System.Net.Http" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.1.1.2" newVersion="4.1.1.2" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.IO.FileSystem" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.2.0" newVersion="4.0.2.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>